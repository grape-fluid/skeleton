  "type": "library",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/grape-fluid/skeleton",
  "authors": [
    {
      "name": "GRAPE SC, a.s.",
      "email": "developers@grapesc.cz",
      "homepage": "https://developers.grapesc.cz"
    }
  ],
  "require": {
    "php": ">= 5.6",
    "nette/finder": "^2.4",
    "nette/neon": "^2.4"
  },
  "autoload": {
    "psr-4": {
